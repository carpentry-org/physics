(load "Test.carp")
(use Test)

(load "physics.carp")
(use Physics)

(defn main []
  (with-test test
    (assert-equal test
                  18.0
                  (momentum 9.0 2.0)
                  "momentum 9.0 2.0 == 18.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  0.0
                  (momentum 0.0 2.0)
                  "momentum 0.0 2.0 == 0.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  18.0
                  (momentum -9.0 -2.0)
                  "momentum -9.0 -2.0 == 18.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  2.0
                  (momentum -1.0 -2.0)
                  "momentum -1.0 -2.0 == 2.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  -18.0
                  (momentum -9.0 2.0)
                  "momentum -9.0 2.0 == -18.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  -2.0
                  (momentum -1.0 2.0)
                  "momentum -1.0 2.0 == -2.0"
                  Double.=
                  Double.str
    )
    (assert-equal test
                  49.05
                  (force 5.0)
                  "force 5.0 == 49.05"
                  Double.approx
                  Double.str
    )
    (assert-equal test
                  88.29
                  (force 9.0)
                  "force 9.0 == 88.29"
                  Double.approx
                  Double.str
    )
    (assert-equal test
                  -49.05
                  (force -5.0)
                  "force -5.0 == -49.05"
                  Double.approx
                  Double.str
    )
    (assert-equal test
                  -88.29
                  (force -9.0)
                  "force -9.0 == -88.29"
                  Double.approx
                  Double.str
    )
